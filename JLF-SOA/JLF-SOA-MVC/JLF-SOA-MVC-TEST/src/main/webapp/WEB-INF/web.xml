<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
  version="3.1"
  metadata-complete="true">

	<servlet>
		<servlet-name>JLF</servlet-name>
		<servlet-class>org.jlf.soa.mvc.web.servlet.JLFMVCServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>JLF</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>init</servlet-name>
		<servlet-class>org.jlf.soa.mvc.test.start.StartServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>init</servlet-name>
		<url-pattern>/a</url-pattern>
	</servlet-mapping>
	<filter>
		<filter-name>sessionFilter</filter-name>
		<filter-class>org.jlf.soa.mvc.web.filter.JLFMVCSessionFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>sessionFilter</filter-name>
		<url-pattern>/session</url-pattern>
	</filter-mapping>
	<filter>
		<filter-name>paramsFilter</filter-name>
		<filter-class>org.jlf.soa.mvc.web.filter.JLFMVCParamsFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>paramsFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
</web-app>
